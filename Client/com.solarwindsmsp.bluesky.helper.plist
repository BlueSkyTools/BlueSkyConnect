<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<dict>
		<key>PathState</key>
		<dict>
			<key>/var/bluesky/.getHelp</key>
			<true/>
		</dict>
	</dict>
	<key>Label</key>
	<string>com.solarwindsmsp.bluesky.helper</string>
	<key>Program</key>
	<string>/var/bluesky/helper.sh</string>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/var/bluesky/helper.stdout</string>
	<key>StandardOutPath</key>
	<string>/var/bluesky/helper.stdout</string>
	<key>StartInterval</key>
	<integer>300</integer>
</dict>
</plist>
